{
    "env": {
        "browser": true,
        "es6": true
    },
    "extends": [
        "plugin:react/recommended",
        "airbnb"
    ],
    "globals": {
        "Atomics": "readonly",
        "SharedArrayBuffer": "readonly"
    },
    "parserOptions": {
        "ecmaFeatures": {
            "jsx": true
        },
        "ecmaVersion": 2018,
        "sourceType": "module"
    },
    "plugins": [
        "react"
    ],
    "rules": {
        "complexity": ["error",2],
        "max-len": ["error", 80],
        "react/jsx-filename-extension": "off",
        "import/no-named-as-default-member": "off",
        "import/no-named-as-default": "off",
        "import/prefer-default-export": "off"
    }
}